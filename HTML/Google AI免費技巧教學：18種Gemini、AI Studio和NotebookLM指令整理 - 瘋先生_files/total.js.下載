th.abs(node.offsetWidth/node.offsetHeight-source.width/source.height)<=.01}init_bem();init_data();init_dom();init_image();setTimeout(function(){$$(".video").forEach(initVideo)});var PLAYER="player";function toggleAudio(videoBox){var videoNode=videoBox[PLAYER],muted=videoNode.muted;videoNode.muted=!muted,modBem(videoBox,"audio",muted)}function loadVideoSource(videoNode,src,callback,errback){videoNode.muted=!0,videoNode.oncanplaythrough=callback,videoNode.setAttribute("playsinline","");var sourceNode=document.createElement("source");return sourceNode.src=src,sourceNode.onerror=errback||callback,videoNode.appendChild(sourceNode),videoNode.load(),videoNode}function initVideo(videoBox){var data4=getData(videoBox),videoData=data4&&data4.video;if(keepOrRemove(videoBox,videoData)){var videoNode=$in(videoBox,"video");videoBox[PLAYER]=videoNode,modBem(videoNode,"cover",!!data4.cover),loadVideoSource(videoNode,getImage(videoData,{node:videoNode,cover:data4.cover}).url),on(keepOrRemove($in(videoBox,".video__audio"),data4.audio),"click",toggleAudio.bind(null,videoBox))}}})();
